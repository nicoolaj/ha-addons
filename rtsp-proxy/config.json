{
  "name": "RTSP Simple Proxy (mediamtx)",
  "version": "1.1.0",
  "slug": "rtsp-proxy",
  "description": "A simple proxy for your RTSP camera streams.",
  "arch": ["armv7", "aarch64", "amd64"],
  "init": false,
  "ports": {
    "8554/tcp": 8554,
    "8888/tcp": 8888
  },
  "ports_description": {
    "8554/tcp": "Main RTSP proxy port.",
    "8888/tcp": "Mediamtx web interface."
  },
  "map": ["addons:rw"],
  "options": {
    "cameras": []
  },
  "schema": {
    "cameras": [
      {
        "name": "str?",
        "url": "str?"
      }
    ]
  },
  "boot": "auto",
  "startup_dependencies": []
}
